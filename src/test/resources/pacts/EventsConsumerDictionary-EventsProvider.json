{
    "provider": {
        "name": "EventsProvider"
    },
    "consumer": {
        "name": "EventsConsumerDictionary"
    },
    "interactions": [
        {
            "description": "a request to get events",
            "request": {
                "method": "GET",
                "path": "/dictionary",
                "headers": {
                    "Accept": "application/json",
                    "Connection": "Keep-Alive",
                    "User-Agent": "Apache-HttpClient/4.5.2 (Java/1.8.0_60)",
                    "Content-Length": "0"
                }
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json; charset=utf-8"
                },
                "body": {
                    "ant": {
                        "title": "ant"
                    }
                },
                "matchingRules": {
                    "$.headers.Content-Type": {
                        "regex": "application/json; charset=utf-8"
                    },
                    "$.body.*": {
                        "match": "type"
                    },
                    "$.body.*.title": {
                        "match": "type"
                    }
                }
            },
            "providerState": "initialStateForEventsTest"
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "2.0.0"
        },
        "pact-jvm": {
            "version": "3.3.7"
        }
    }
}