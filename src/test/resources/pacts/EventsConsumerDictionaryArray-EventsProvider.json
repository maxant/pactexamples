{
    "provider": {
        "name": "EventsProvider"
    },
    "consumer": {
        "name": "EventsConsumerDictionaryArray"
    },
    "interactions": [
        {
            "description": "a request to get events keyed by title",
            "request": {
                "method": "GET",
                "path": "/dictionaryArray",
                "headers": {
                    "Accept": "application/json"
                }
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json; charset=utf-8"
                },
                "body": {
                    "ant": [
                        {
                            "title": "ant"
                        }
                    ]
                },
                "matchingRules": {
                    "$.headers.Content-Type": {
                        "regex": "application/json; charset=utf-8"
                    },
                    "$.body.*": {
                        "min": 0,
                        "match": "type"
                    },
                    "$.body.*[*].title": {
                        "match": "type"
                    }
                }
            },
            "providerState": "initialStateForEventsTest"
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "2.0.0"
        },
        "pact-jvm": {
            "version": "3.3.7"
        }
    }
}